{
  "name": "xaiagentplatform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:dev": "next build ",
    "build:prod": "next build",
    "start:dev": "NODE_ENV=development node server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "redeploy:dev": "git pull && pnpm install && pnpm run build:dev && pm2 restart xaiagentplatform-dev",
    "redeploy:prod-old": "git pull &&  pnpm build:prod && PORT=3001 NODE_ENV=production node server.js",
    "redeploy:prod": "git pull &&  pnpm build:prod && pm2 start ecosystem.config.js --only xaiagentplatform-prod",
    "redeploy:beta": "git pull && pnpm install && pnpm run build:prod && pm2 start ecosystem.config.js --only xaiagentplatform",
    "lint": "next lint",
    "update-prices": "ts-node -P scripts/tsconfig.json scripts/update-prices.ts",
    "update-prices:dev": "NODE_ENV=development ts-node -P scripts/tsconfig.json scripts/update-prices.ts",
    "update-prices:prod": "NODE_ENV=production ts-node -P scripts/tsconfig.json scripts/update-prices.ts",
    "sync-agents": "ts-node -P scripts/tsconfig.json scripts/sync-agents.ts",
    "clear-agents": "ts-node -P scripts/tsconfig.json scripts/clear-agents.ts",
    "prisma:seed": "ts-node -P scripts/tsconfig.json prisma/seed.ts"
  },
