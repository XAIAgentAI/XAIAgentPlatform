{
  "name": "xaiagentplatform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm prisma generate && node start-dev.js",
    "build:dev": "DOTENV_CONFIG_PATH=.env.development next build",
    "build:test": "NODE_ENV=test next build",
    "build:prod": "NODE_ENV=production next build",
    "start:dev": "NODE_ENV=development node server.js",
    "start:test": "NODE_ENV=test node server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "redeploy:dev": "git pull && pnpm install && pnpm run build:dev && pm2 restart xaiagentplatform-dev",
    "redeploy:test": "git pull && pnpm install && pnpm run build:test && pm2 restart xaiagentplatform",
    "redeploy:prod-old": "git pull &&  pnpm build:prod && PORT=3001 NODE_ENV=production node server.js",
    "redeploy:prod": "git pull &&  pnpm build:prod && pm2 start ecosystem.config.js --only xaiagentplatform-prod",
    "redeploy:beta": "git pull && pnpm install && pnpm run build:prod && pm2 start ecosystem.config.js --only xaiagentplatform",
    "lint": "next lint",
    "update-prices": "ts-node -P scripts/tsconfig.json scripts/update-prices.ts",
    "update-prices:dev": "NODE_ENV=development ts-node -P scripts/tsconfig.json scripts/update-prices.ts",
    "update-prices:test": "NODE_ENV=test ts-node -P scripts/tsconfig.json scripts/update-prices.ts",
    "update-prices:prod": "NODE_ENV=production ts-node -P scripts/tsconfig.json scripts/update-prices.ts",
    "sync-agents": "ts-node -P scripts/tsconfig.json scripts/sync-agents.ts",
    "clear-agents": "ts-node -P scripts/tsconfig.json scripts/clear-agents.ts",
    "prisma:seed": "ts-node -P scripts/tsconfig.json prisma/seed.ts",
    "test:agent-api": "tsx scripts/test-agent-api.ts",
    "delete:test-agents": "tsx scripts/delete-test-agents.ts",
    "memory:monitor": "node scripts/memory-monitor.js",
    "memory:monitor:continuous": "node scripts/memory-monitor.js --continuous",
    "memory:monitor:save": "node scripts/memory-monitor.js --continuous --save",
    "build:debug": "next build --experimental-debug-memory-usage",
    "build:profile": "node --heap-prof node_modules/next/dist/bin/next build",
    "sync-translations": "node sync-keys-with-zh.js && node scripts/sync-translations.js --export-pending && echo \"\" && echo \"@pending-translation.en.json @pending-translation.ja.json @pending-translation.ko.json\" && echo \"\" && echo \"我希望你帮我做翻译工作。你根据zh.json这里面对应这里面的key对应的value值，去对每一个key进行翻译，然后的然后更新这三个pending文件。\" && echo \"\"",
    "apply-translations": "node scripts/sync-translations.js --import-pending && echo \"\" &&  rm -f messages/pending-translation.en.json messages/pending-translation.ja.json messages/pending-translation.ko.json ",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:unit": "vitest run tests/api/",
    "test:integration": "vitest run tests/integration/ --config vitest.integration.config.ts",
    "test:e2e": "tsx scripts/test-deploy-mining-e2e.ts",
    "test:all": "pnpm run test:unit && pnpm run test:integration && pnpm run test:e2e",
    "test:watch": "vitest tests/api/",
    "deploy:sync:test": "rsync -avz --delete --exclude=.git --exclude=cache ./.next/standalone/ ubuntu@52.78.26.103:~/projects/XAIAgentPlatform/ && rsync -avz ./.next/static/ ubuntu@52.78.26.103:~/projects/XAIAgentPlatform/.next/static/ && rsync -avz ./ecosystem.config.js ./prisma/ ubuntu@52.78.26.103:~/projects/XAIAgentPlatform/ && rsync -avz ./.env.test ubuntu@52.78.26.103:~/projects/XAIAgentPlatform/.env",
    "deploy:sync:prod": "rsync -avz --delete --exclude=node_modules --exclude=.git --exclude=cache --exclude=src ./.next/standalone/ ubuntu@52.78.26.103:~/projects/XAIAgentPlatform/ && rsync -avz ./.next/static ubuntu@52.78.26.103:~/projects/XAIAgentPlatform/.next/ && rsync -avz package.json ecosystem.config.js prisma/ ubuntu@52.78.26.103:~/projects/XAIAgentPlatform/ && rsync -avz .env.production ubuntu@52.78.26.103:~/projects/XAIAgentPlatform/.env",
    "local-deploy-test": "pnpm prisma generate && pnpm run build:test && pnpm run deploy:sync:test && echo '文件同步完成，请手动SSH到服务器重启服务'",
    "local-deploy-prod": "pnpm run build:prod && pnpm run deploy:sync:prod && echo '文件同步完成，请手动SSH到服务器重启服务'"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.10.0",
    "@neondatabase/serverless": "^0.10.4",
    "@prisma/client": "6.2.1",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@reown/appkit": "^1.6.9",
    "@reown/appkit-adapter-wagmi": "^1.6.9",
    "@tanstack/react-query": "^5.64.2",
    "@types/ali-oss": "^6.16.11",
    "@types/dotenv": "^8.2.3",
    "@types/uuid": "^10.0.0",
    "@uniswap/sdk-core": "^7.7.2",
    "@uniswap/v3-sdk": "^3.25.2",
    "ali-oss": "^6.22.0",
    "axios": "^1.7.9",
    "bignumber.js": "^9.1.2",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.5.1",
    "clsx": "^2.1.1",
    "country-flag-icons": "^1.5.18",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.5",
    "framer-motion": "^12.0.3",
    "jose": "^5.9.6",
    "jsbi": "^4.3.2",
    "jsonwebtoken": "^9.0.2",
    "lightweight-charts": "^4.2.2",
    "lodash.clamp": "^4.0.3",
    "lucide-react": "^0.469.0",
    "next": "14.0.4",
    "next-intl": "^3.26.3",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-markdown": "^10.1.0",
    "react-use": "^17.6.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.1",
    "tailwind-merge": "^2.6.0",
    "uuid": "^11.1.0",
    "vconsole": "^3.15.1",
    "viem": "^2.22.13",
    "wagmi": "^2.14.9",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@shadcn/ui": "^0.0.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash.clamp": "^4.0.9",
    "@types/node": "^20.17.12",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/ui": "^3.2.4",
    "@wagmi/core": "^2.16.3",
    "autoprefixer": "^10.4.20",
    "c8": "^10.1.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-react": "^7.37.3",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.4.49",
    "prisma": "^6.15.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vite": "^7.1.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
